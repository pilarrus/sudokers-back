{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "arrow-parens": "off",
      "camelcase": "off",
      "comma-dangle": "off",
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "object-curly-newline":"off",
      "spaced-comment": "off"
    }
}